            "name": "bigiqVMNSG",
            "apiVersion": "2015-06-15",
            "location": "usgovvirginia",
            "tags": {},
            "properties": {
                "securityRules": [
                    {
                        "name": "default-allow-ssh",
                        "properties": {
                            "access": "Allow",
                            "direction": "Inbound",
                            "protocol": "Tcp",
                            "priority": 1000,
                            "destinationPortRange": "22",
